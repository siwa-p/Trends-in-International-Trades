{
  "name": "Python Base DevContainer",
  "dockerComposeFile": [
    "compose-services/docker-compose.base.yml",
    "compose-services/docker-compose.postgres.yml",
    "compose-services/docker-compose.nessie.yml",
    "compose-services/docker-compose.minio.yml",
    "compose-services/docker-compose.dremio.yml"
  ],
  "service": "app",
  "workspaceFolder": "/workspace",
  "features": {
    "ghcr.io/devcontainers/features/docker-from-docker:1": {}
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance",
        "mechatroner.rainbow-csv",m 
        "redhat.vscode-yaml",
        "GitHub.copilot",
        "GitHub.copilot-chat"
      ]
    }
  }
}
